{
  "manifest_version": 3,
  "name": "Course Analyzer",
  "version": "1.0",
  "description": "Анализирует страницы курсов и показывает информацию",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}