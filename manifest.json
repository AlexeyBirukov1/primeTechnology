{
  "manifest_version": 3,
  "name": "Course Analyzer",
  "version": "1.0",
  "description": "Анализирует страницы курсов и показывает информацию",
  "permissions": ["activeTab", "http://127.0.0.1:8000/*"],
  "background": {
    "service_worker": "bg.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}