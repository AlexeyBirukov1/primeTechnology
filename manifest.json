{
  "manifest_version": 3,
  "name": "СравниКурс",
  "version": "1.0",
  "description": "Использование динамичной базы данных и продвинутых ИИ технологий для выбора лучших онлайн-курсов!",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}